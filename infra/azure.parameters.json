{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "resourceBaseName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION=eastus}"
      },
      "botAadAppClientId": {
        "value": "${botAadAppClientId}"
      },
      "botAadAppClientSecret": {
        "value": "${botAadAppClientSecret}"
      },
      "botAadAppTenantId": {
        "value": "${botAadAppTenantId}"
      },
      "botDisplayName": {
        "value": "TeamsConversationBot"
      },
      "botServiceSku": {
        "value": "${BOT_SERVICE_SKU=S1}"
      },
      "appServicePlanSku": {
        "value": "${APP_SERVICE_PLAN_SKU=B1}"
      },
      "azureOpenAiApiKey": {
        "value": "${AZURE_OPENAI_API_KEY}"
      },
      "azureOpenAiEndpoint": {
        "value": "${AZURE_OPENAI_ENDPOINT}"
      },
      "azureOpenAiModel": {
        "value": "${AZURE_OPENAI_MODEL}"
      }
    }
  }